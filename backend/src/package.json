{
  "name": "app-backend",
  "version": "1.0.0",
  "description": "AppBackend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs --watch \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t toto .",
    "docker:run": "docker run -p 3000:3000 -d toto",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "nodemon --ext js,ts --watch src --watch config --exec \"node -r source-map-support/register .\" --exec \"npm run start:prod\"",
    "prepublishOnly": "npm run test",
    "k8s-run": "npm run start:prod",
    "start:prod": "npm run build && node ."
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.2.0",
    "@loopback/boot": "^1.5.11",
    "@loopback/context": "^1.23.5",
    "@loopback/core": "^1.10.7",
    "@loopback/openapi-v3": "^1.10.1",
    "@loopback/repository": "^1.15.4",
    "@loopback/rest": "^1.23.0",
    "@loopback/rest-explorer": "^1.4.4",
    "@loopback/service-proxy": "^1.3.11",
    "@types/config": "0.0.34",
    "@types/csv-parse": "^1.1.12",
    "@types/javascript-state-machine": "^2.4.1",
    "@types/md5-file": "^4.0.0",
    "@types/multiparty": "0.0.31",
    "@types/nodemailer": "^4.6.7",
    "@types/papaparse": "^4.5.9",
    "@types/passport-jwt": "^3.0.1",
    "@types/pg": "^7.4.14",
    "@types/pg-format": "^1.0.0",
    "@types/uuid": "^3.4.4",
    "arraybuffer-to-string": "^1.0.2",
    "config": "^3.1.0",
    "cross-env": "^5.2.0",
    "csv-parse": "^4.4.1",
    "fabric-client": "^1.4.4",
    "fabric-network": "^1.4.4",
    "highlight.js": "9.14.2",
    "html-tablify": "^2.0.2",
    "javascript-state-machine": "^3.1.0",
    "jwks-rsa": "^1.3.0",
    "log4js": "^3.0.6",
    "loopback-connector-postgresql": "^3.8.0",
    "loopback-datasource-juggler": "^4.13.0",
    "md5-file": "^4.0.0",
    "moment": "^2.24.0",
    "multiparty": "^4.2.1",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.11",
    "papaparse": "^4.6.3",
    "passport-jwt": "^4.0.0",
    "passport-mock-strategy": "^1.1.1",
    "pg": "^7.11.0",
    "pg-format": "^1.0.4",
    "socket.io": "^2.2.0",
    "strip-bom-stream": "^3.0.0",
    "superagent": "^4.1.0",
    "uuid": "^3.4.0",
    "yaml-js": "^0.2.3"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.16",
    "@loopback/eslint-config": "^4.1.4",
    "@loopback/testlab": "^1.9.4",
    "@loopback/tslint-config": "^1.0.0",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^10.17.5",
    "@types/socket.io": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.1",
    "mock-fs": "^4.9.0",
    "prettify": "^0.1.7",
    "source-map-support": "^0.5.16",
    "ts-mockito": "^2.5.0",
    "tslint": "^5.20.1",
    "typescript": "~3.7.2"
  }
}
